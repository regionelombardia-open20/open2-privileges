{
    "name": "lispa/amos-privileges",
    "description": "AMOS plugin for privileges management (role, permission, domains.. )",
    "keywords": ["amos", "yii2", "privileges"],
    "homepage": "http://www.lispa.it/",
    "type": "component",
    "proscription": "BSD-3-Clause",
    "require": {
        "php": ">=5.4.0",
        "lispa/amos-admin" : "*",
        "lispa/amos-core" : "*"
    },
    "config": {
        "process-timeout": 1800,
        "secure-http": false
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "autoload": {
        "psr-4": {
            "lispa\\amos\\privileges\\": "src"
        }
    },
    "require-dev": {
        "yiisoft/yii2-faker": "*"
    }
}
